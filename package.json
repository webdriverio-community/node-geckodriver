{
  "name": "geckodriver",
  "version": "5.0.0",
  "description": "Mozilla's Geckodriver for Node.js",
  "homepage": "https://github.com/webdriverio-community/node-geckodriver#readme",
  "license": "MIT",
  "author": {
    "name": "vladikoff",
    "email": "vlad.filippov@gmail.com",
    "url": "http://vf.io"
  },
  "contributors": [
    {
      "name": "Christian Bromann",
      "url": "https://bromann.dev",
      "email": "mail@bromann.dev"
    }
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "geckodriver",
    "webdriver"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webdriverio-community/node-geckodriver.git"
  },
  "scripts": {
    "build": "tsc -b .",
    "clean": "rimraf ./node_modules package-lock.yaml ./dist",
    "prepare": "husky install",
    "postinstall": "test -f ./dist/install.js && node ./dist/install.js || echo \"Skipping install, project not build!\"",
    "test": "run-s build test:*",
    "test:lint": "eslint",
    "test:unit": "vitest --run",
    "test:e2e": "tsx ./tests/test.e2e.ts",
    "watch": "npm run build -- --watch"
  },
  "bin": {
    "geckodriver": "./bin/geckodriver.js"
  },
  "bugs": {
    "url": "https://github.com/webdriverio-community/node-geckodriver/issues"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": [
      {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/cjs/index.js"
      },
      "./dist/cjs/index.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/tar-fs": "^2.0.4",
    "@vitest/coverage-v8": "^3.0.2",
    "@wdio/eslint": "^0.1.2",
    "eslint": "^9.16.0",
    "geckodriver": ".",
    "husky": "^9.1.7",
    "npm-run-all2": "^7.0.1",
    "octokit": "^4.0.2",
    "release-it": "^19.0.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^3.0.2",
    "wait-port": "^1.1.0",
    "webdriverio": "^9.4.1"
  },
  "dependencies": {
    "@wdio/logger": "^9.1.3",
    "@zip.js/zip.js": "^2.7.54",
    "decamelize": "^6.0.0",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.5",
    "node-fetch": "^3.3.2",
    "tar-fs": "^3.0.6"
  }
}
